{
    "name"        : "SimpTab",
    "version"     : "1.0.0",
    "description" : "Minimalistic's chrome  new tab page extension, hope every time open, you have a good mood.",
    "homepage_url": "https://github.com/kenshin/simptab",
    "chrome_url_overrides": {
        "newtab": "main.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "storage",
        "*://*/bing.com*"
    ],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 2
}